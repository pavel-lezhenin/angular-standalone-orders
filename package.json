{
  "name": "@front-templates/angular-standalone-orders",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "start": "ng serve",
    "build": "ng build",
    "preview": "ng serve --configuration=production",
    "lint": "eslint src --ext ts,html --fix",
    "lint:check": "eslint src --ext ts,html",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,html,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,html,json}\"",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "storybook": "ng run angular-standalone-orders:storybook",
    "storybook:build": "ng run angular-standalone-orders:build-storybook",
    "validate:design-system": "node scripts/validate-design-system.js",
    "lint:styles": "npm run validate:design-system",
    "serve:ssr": "node dist/angular-standalone-orders/server/server.mjs",
    "prepare": "husky install"
  },
  "private": true,
  "engines": {
    "node": ">=20 <24"
  },
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "overrides": {
      "webpack": "5.105.2"
    }
  },
  "dependencies": {
    "@angular/animations": "^21.1.0",
    "@angular/cdk": "^21.1.3",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/material": "^21.1.3",
    "@angular/platform-browser": "^21.1.0",
    "@angular/platform-server": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@angular/ssr": "^21.1.3",
    "compression": "^1.7.4",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^21.0.0",
    "@angular-eslint/eslint-plugin": "^21.0.0",
    "@angular-eslint/eslint-plugin-template": "^21.0.0",
    "@angular-eslint/schematics": "^21.0.0",
    "@angular-eslint/template-parser": "^21.0.0",
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/angular": "^10.2.8",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "msw": "^2.4.0",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "storybook": "^10.2.8",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "zone.js": "^0.16.0"
  }
}